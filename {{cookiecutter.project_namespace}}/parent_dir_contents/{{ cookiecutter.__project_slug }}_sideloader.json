    },
    "namespace": "{{ cookiecutter.project_namespace }}.{{ cookiecutter.__project_slug }}",
    "path": [
        "{{ cookiecutter.project_namespace }}",
        "{{ cookiecutter.__project_slug }}",
        "__init__.py"
